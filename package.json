{
  "name": "inkton-nest",
  "displayName": "inkton.nest",
  "description": "to manage nest local work",
  "version": "0.0.1",
  "publisher": "Inkton",
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.inkton-nest.dataup",
    "onCommand:extension.inkton-nest.datadown",
    "onCommand:extension.inkton-nest.viewdata",
    "onCommand:extension.inkton-nest.viewqueue",    
    "onCommand:extension.inkton-nest.select",
    "onCommand:extension.inkton-nest.clean",
    "onCommand:extension.inkton-nest.clear",    
    "onCommand:extension.inkton-nest.reset",    
    "onCommand:extension.inkton-nest.build",
    "onCommand:extension.inkton-nest.publish",    
    "onCommand:extension.inkton-nest.restore",    
    "onCommand:extension.inkton-nest.scaffold",
    "onCommand:extension.inkton-nest.help"    
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.inkton-nest.dataup",
        "title": "Nest Data Up"
      },
      {
        "command": "extension.inkton-nest.datadown",
        "title": "Nest Data Down"
      },
      {
        "command": "extension.inkton-nest.viewdata",
        "title": "Nest View Data"
      },
      {
        "command": "extension.inkton-nest.viewqueue",
        "title": "Nest View Queue"
      },
      {
        "command": "extension.inkton-nest.select",
        "title": "Nest Select"
      },
      {
        "command": "extension.inkton-nest.clean",
        "title": "Nest Clean"
      },
      {
        "command": "extension.inkton-nest.clear",
        "title": "Nest Clear"
      },      
      {
        "command": "extension.inkton-nest.reset",
        "title": "Nest Reset"
      },      
      {
        "command": "extension.inkton-nest.build",
        "title": "Nest Build"
      },
      {
        "command": "extension.inkton-nest.publish",
        "title": "Nest Publish"
      },      
      {
        "command": "extension.inkton-nest.restore",
        "title": "Nest Restore"
      },
      {
        "command": "extension.inkton-nest.scaffold",
        "title": "Nest Scaffold"
      },
      {
        "command": "extension.inkton-nest.help",
        "title": "Nest Help"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32",
    "path-exists": "3.0.0",
    "yamljs": "0.2.10",
    "xml2js": "0.4.17",
    "q": "1.5.0",
    "child-process-promise":"2.2.1"
  }
}
